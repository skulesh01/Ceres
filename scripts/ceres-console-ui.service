[Unit]
Description=CERES Console UI (admin)
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
WorkingDirectory=/opt/ceres/Ceres
EnvironmentFile=-/etc/ceres/console-ui.env
ExecStart=/opt/ceres/Ceres/bin/ceres-ui
Restart=always
RestartSec=2

[Install]
WantedBy=multi-user.target
